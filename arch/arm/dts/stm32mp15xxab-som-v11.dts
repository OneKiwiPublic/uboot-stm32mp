/dts-v1/;

#include "stm32mp157.dtsi"
#include "stm32mp15xa.dtsi"
#include "stm32mp15-pinctrl.dtsi"
#include "stm32mp15xxab-pinctrl.dtsi"
#include "stm32mp15xxab-som-v11-pin.dtsi"
#include "stm32mp15xxab-som-v11.dtsi"
